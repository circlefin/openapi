                        },
                        "fees": {
                          "amount": "3.14",
                          "currency": "USD"
                        },
                        "networkFees": {
                          "amount": "0.14",
                          "currency": "USD"
                        },
                        "status": "pending",
                        "errorCode": "transaction_denied",
                        "riskEvaluation": {
                          "decision": "denied",
                          "reason": "4000"
                        },
                        "createDate": "2020-04-10T02:13:30.000Z",
                        "updateDate": "2020-04-10T02:13:30.000Z"
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
