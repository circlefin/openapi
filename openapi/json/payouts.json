            "enum": [
              "USD",
              "EUR",
              "BTC",
              "ETH",
              "MTC"
            ]
          }
        }
      },
      "CryptoPayoutCreationRequest": {
        "type": "object",
        "required": [
          "idempotencyKey",
          "destination",
          "amount"
        ],
        "properties": {
          "idempotencyKey": {
            "$ref": "#/components/schemas/IdempotencyKey"
          },
